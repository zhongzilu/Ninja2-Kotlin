<?xml version="1.0" encoding="utf-8"?>

<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/action_bar_size_48"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".PageActivity"
    tools:showIn="@layout/activity_page">

    <!--Pin List RecyclerView-->
    <ViewStub
        android:id="@+id/mPinsStub"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:inflatedId="@+id/mPinsRecycler"
        android:layout="@layout/stub_pins_recycler" />

    <!--Custom WebView-->
    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
